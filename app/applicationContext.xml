<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd




           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.1.xsd">

    <context:annotation-config />

    <bean id="dataService" class="com.enterprise.services.DataService">
        <property name="sessionManager" ref="sessionManager"></property>
    </bean>
    <bean id="donorService" class="com.enterprise.services.DonorService">
        <property name="sessionManager" ref="sessionManager"></property>
    </bean>
    <bean id="donationService" class="com.enterprise.services.DonationService">
        <property name="sessionManager" ref="sessionManager"></property>
    </bean>
    <bean id="loginService" class="com.enterprise.services.LoginService">
        <property name="sessionManager" ref="sessionManager"></property>
    </bean>

    <bean id="sessionManager" class="com.enterprise.session.SessionManager">
        <property name="context" ref="androidContext"></property>
    </bean>

</beans>